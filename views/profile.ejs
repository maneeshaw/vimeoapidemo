<title>MY DEMO</title>
<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1">
<link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
<link href="https://fonts.googleapis.com/css?family=Raleway" rel="stylesheet" type="text/css">
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/mdbootstrap/4.5.7/css/mdb.min.css" />
<!-- Material Design Bootstrap -->
<link href="css/mdb.min.css" rel="stylesheet">
<!-- Your custom styles (optional) -->
<link href="css/style.css" rel="stylesheet">
<link rel="stylesheet" type="text/css" href="/wintergarten.css">
<link rel="stylesheet" href="http://fortawesome.github.io/Font-Awesome/assets/font-awesome/css/font-awesome.css">
<link rel="stylesheet" type="text/css" href="/mydemo.css">


<body>

  <!-- !PAGE CONTENT! -->
  <div class="w3-content" style="max-width:1500px">

    <!-- Header -->
    <header class="w3-container w3-xlarge w3-padding-24">
      <a href="/" class="w3-left w3-button w3-white">MY DEMO</a>
      <a href="#about" class="w3-right w3-button w3-white">Search</a>
      <a href="#about" class="w3-right w3-button w3-white">Partners</a>
      <a href="/analytics" class="w3-right w3-button w3-white">Analytics</a>
      <a href="#analytics" class="w3-right w3-button w3-white">Live</a>
      <a href="#about" class="w3-right w3-button w3-white">Shop</a>
    </header>

    <div margin-top="30px">
      <h1 class="padding-bottom-small head-2" align="center"><%= user.displayName %></h1>
      <p class="padding-bottom-small head-2" align="center">
        <%= user.email %>
      </p>
    </div>

    <div class='parent' align="center">
      <div class="child inline-block-child button_cont example_c"><a href="/logout">Log out</a></div>
      <div class="child inline-block-child button_cont example_c"><a href="<%= otturl%>">OTT URL</a></div>
    </div>


    <section data-component="collections" class="padding-large">
      <h2 class="margin-left-medium padding-bottom-small head-2 border--bottom">WatchList</h2>
      <ul class="row padding-bottom-large">
        <% watchlist.forEach(function(list) { %>
          <li class="col small-1-1 medium-1-2 large-1-4 padding-medium">
            <h3 class="head-3 margin-bottom-small truncate"><%= list.name %></h3>
            <a href="/watch/<%= list.id %>" class="ratio ratio--16-9">
            <img src="<%= list.thumbnail.medium %>" class="fill-width"></a>
          </li>
          <% }) %>
      </ul>
    </section>
  </div>
</body>